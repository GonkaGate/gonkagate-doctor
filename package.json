{
  "name": "gonkagate",
  "version": "0.2.1",
  "description": "GonkaGate CLI (MVP: gonkagate doctor)",
  "keywords": [
    "gonkagate",
    "openai",
    "cli",
    "diagnostics",
    "doctor"
  ],
  "homepage": "https://github.com/GonkaGate/gonkagate-doctor#readme",
  "bugs": {
    "url": "https://github.com/GonkaGate/gonkagate-doctor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GonkaGate/gonkagate-doctor.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "bin": {
    "gonkagate": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsup",
    "start": "node dist/cli.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "ci": "npm run format:check && npm run lint && npm run typecheck && npm test && npm run build"
  },
  "dependencies": {
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
